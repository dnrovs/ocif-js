{
    "name": "ocif-js",
    "version": "1.0.0",
    "type": "module",
    "description": "A library for working with OCIF (OpenComputers Image Format) images",
    "author": "dnrovs",
    "license": "MIT",
    "repository": "https://github.com/dnrovs/ocif.js",
    "homepage": "https://dnrovs.github.io/ocif.js",
    "scripts": {
        "build": "rollup -c",
        "docs": "typedoc src",
        "test": "vitest run --coverage",
        "test:dev": "vitest --coverage --ui",
        "lint": "eslint src",
        "format": "prettier -c \"{src,tests}/**/*.ts\""
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "keywords": [
        "ocif",
        "opencomputers",
        "image",
        "minecraft",
        "png",
        "converter"
    ],
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "pngjs": "^7.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@rollup/plugin-commonjs": "^29.0.0",
        "@rollup/plugin-typescript": "^12.3.0",
        "@types/node": "^25.0.3",
        "@types/pngjs": "^6.0.5",
        "@typescript-eslint/eslint-plugin": "^8.50.1",
        "@typescript-eslint/parser": "^8.50.1",
        "@vitest/coverage-v8": "^4.0.16",
        "@vitest/ui": "^4.0.16",
        "eslint": "^9.39.2",
        "globals": "^16.5.0",
        "prettier": "^3.7.4",
        "rollup": "^4.54.0",
        "rollup-plugin-copy": "^3.5.0",
        "rollup-plugin-dts": "^6.3.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.1",
        "typedoc": "^0.28.15",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.50.1",
        "vitest": "^4.0.16"
    }
}
